{
    "groups": {
        "voc_átona": ["a", "e", "i", "o", "u"],
        "voc_tónica": ["á", "é", "í", "ó", "ú"],
        "voc": ["<voc_átona>", "<voc_tónica>"],
        "ei_átona": ["e", "i"],
        "ei_tónica": ["é", "í"],
        "ei": ["<ei_átona>", "<ei_tónica>"],
        "ao_átona": ["a", "o"],
        "ao_tónica": ["á", "ó"],
        "ao": ["<ao_átona>", "<ao_tónica>"],
        "cons": ["b", "c", "d", "f", "g", "h", "j", "k", "l", "m", "n", "ñ", "p", "q", "r", "s", "t", "v", "w", "x", "y", "z"],
        "bilabial|bil": ["p", "b", "m", "β"],
        "labiodental|lab": ["ɱ", "f", "v"],
        "velar|vel": ["k", "g", "ŋ", "x", "ɣ"],
        "sonora|son": ["b", "d", "g", "m", "ɱ", "n", "ŋ", "r", "ɾ", "β", "v", "ð", "z", "ʝ", "ɣ", "j", "l", "ʎ"],
        "r_no_n_s": ["<cons>", "<voc>", "!n", "!s"], // Cualquier sonido excepto n o s
        "r_l": ["ɾ", "l"] // r suave o l
    },
    "rules": {
        "iden": {"": ""}, // Identidad
        // Reglas básicas
        "c": {
            "c<ei>": "θ<ei>",
            "c": "k"
        },
        "ch": {"ch": "tʃ"},
        "g": {
            "gü<ei>": "gw<ei>",
            "gu<ei>": "g<ei>",
            "g<ei>": "x<ei>"
        },
        "h": {"h": ""},
        "j": {"j": "x"},
        "ll": {"ll": "ʎ"},
        "ñ": {"ñ": "ɲ"},
        "q": {
            "qu<ei>": "k<ei>",
            "q": "k"
        },
        "r_suave": {"<r_no_n_s>r": "<r_no_n_s>ɾ"}, // Necesario para que no convierta las r de inicio de palabra
        "r_fuerte": {"rr": "r"},
        "v": {"v": "b"},
        "w_tradicional": {"w": "b"},
        "w_moderna": {"w": "gw"},
        "x": {"x": "ks"},
        "y": {
            "y<voc>": "ʝ<voc>",
            "y": "i"
        },
        "z": {"z": "θ"},
        "i": {"i<voc>": "j<voc>"},
        "u": {"u<voc>": "w<voc>"},
        // Alófonos
        "fricativización": {
            "<voc|r_l>b<voc|r_l>": "<voc|r_l>β<voc|r_l>",
            "<voc|r_l>d<voc|r_l>": "<voc|r_l>ð<voc|r_l>",
            "<voc|r_l>g<voc|r_l>": "<voc|r_l>ɣ<voc|r_l>"
        },
        "n_asimilada": {
            "n<bil>": "m<bil>",
            "n<lab>": "ɱ<lab>",
            "n<vel>": "ŋ<vel>"
        },
        "s_sonora": {"s<son>": "z<son>"}
    },
    "order": [
        "ch", "ll", "r_fuerte", "x", "w_tradicional|w_moderna", // Primero digrafos y sonidos dobles
        "c", "g", "h", "j", "ñ", "q", "r_suave", "v", "y", "z", "i", "u",  // Luego letras
        "fricativización", "n_asimilada", "s_sonora" // Alófonos
    ]
}